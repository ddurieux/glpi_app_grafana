{
  "type": "datasource",
  "name": "glpi2",
  "id": "glpi-glpi",
  "metrics": true,
  "info": {
    "description": "Plugin GLPI description",
    "author": {
      "name": "David Durieux",
      "url": "http://nothing"
    },
    "keywords": [
      "glpi"
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/grafana/simple-datasource"
      },
      {
        "name": "License",
        "url": "https://github.com/grafana/simple-datasource/blob/master/LICENSE"
      }
    ],
    "screenshots": [],
    "version": "1.0.0",
    "updated": "2020-10-06"
  },
  "dependencies": {
    "grafanaVersion": "6.5.x",
    "plugins": []
  },
  "routes": [
    {
      "path": "initSession",
      "url": "{{ .JsonData.url }}/initSession",
      "headers": [
        {
          "name": "App-Token",
          "content": "{{ .SecureJsonData.appToken }}"
        },
        {
          "name": "Authorization",
          "content": "user_token {{ .SecureJsonData.userToken }}"
        }
      ]
    },
    {
      "path": "query",
      "url": "{{ .JsonData.url }}",
      "headers": [
        {
          "name": "App-Token",
          "content": "{{ .SecureJsonData.appToken }}"
        }
      ]
    }
  ]
  
}
